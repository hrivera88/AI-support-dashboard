{
  "name": "ai-support-dashboard-server",
  "version": "1.0.0",
  "description": "AI-Powered Customer Support Dashboard Backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && webpack --mode production",
    "build:lambda": "webpack --mode production",
    "start": "node dist/index.js",
    "start:lambda": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.0",
    "typescript": "^5.0.0",
    "tsx": "^4.7.0",
    "openai": "^4.24.0",
    "@pinecone-database/pinecone": "^1.1.0",
    "redis": "^4.6.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "@vendia/serverless-express": "^4.10.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.11.0",
    "@types/aws-lambda": "^8.10.131",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "serverless-webpack": "^5.13.0",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0",
    "ts-loader": "^9.5.1"
  }
}